    "version": "0.2.0",
    "configurations": [
        {
            "name": "Overfit",
            "type": "debugpy",
            "request": "launch",
            "module": "flowmap.overfit",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "wandb.mode=disabled",
                "trainer.max_steps=500",
                "track_precomputation.cache_path=/tmp/flowmap/track_cache",
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
            }
        },
        {
            "name": "Pretrain",
            "type": "debugpy",
            "request": "launch",
            "module": "flowmap.pretrain",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "wandb.mode=disabled",
                "data_module.train.num_workers=0",
                "data_module.val.num_workers=0",
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
            }
        },
        {
            "name": "Subsample Dataset",
            "type": "debugpy",
            "request": "launch",
            "module": "flowmap.subsample",
            "console": "integratedTerminal",
